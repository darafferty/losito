# Example parset
ncpu = 0

[beam]
operation = BEAM
mode = full

[tec1]
operation = TEC
method = fits
fitsFilename = example.fits
h5parmFilename = tec1.h5

[tec2]
operation = TEC
method = tid
tidAmp = 0.3
h5parmFilename = tec2.h5

[predict]
operation = PREDICT
outputColumn = CORRECTED_DATA

[noise]
operation = NOISE
column = CORRECTED_DATA
